/* pages/habit-find/find-total-rank/index.wxss */
@import '/style/weui-rpx.wxss';

@font-face {
  font-family: 'iconfont';  /* project id 1282975 */
  src: url('//at.alicdn.com/t/font_1282975_mjzcksxppps.eot');
  src: url('//at.alicdn.com/t/font_1282975_mjzcksxppps.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1282975_mjzcksxppps.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1282975_mjzcksxppps.woff') format('woff'),
  url('//at.alicdn.com/t/font_1282975_mjzcksxppps.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1282975_mjzcksxppps.svg#iconfont') format('svg');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 36rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.iconwomen:before {
  content: "\e632";
}

.iconman:before {
  content: "\e631";
}

.ranking-top-text {
  font-size: 24rpx;
  font-family:PingFang-SC-Medium;
  line-height: 60rpx;
  padding: 0 30rpx;
  margin-top: 30rpx;
}
.ranking-top-text-left {
  float: left;
  color: #999;
}

.ranking-top-text-right {
  float: right;
}


.sanjiao_down{
  width:0;
  height:0;
  overflow:hidden;
  font-size: 0;     /*是因为, 虽然宽高度为0, 但在IE6下会具有默认的 */
  line-height: 0;  /* 字体大小和行高, 导致盒子呈现被撑开的长矩形 */
  border-width:15rpx;
  border-style:dashed dashed solid dashed;/*IE6下, 设置余下三条边的border-style为dashed,即可达到透明的效果*/
  border-color:transparent transparent #FEFEFE transparent;
}

.ranking-me {
  height: 120rpx;
  background: #fff;
  clear: both;
}
.ranking-me-bottom {
  margin-bottom: 20rpx;
}

.ranking-me .ranking-me-left-img {
  display: inline-block;
  margin-left: 22rpx;
  vertical-align: top;
  margin-top: 20rpx;
}

.ranking-me .ranking-me-left-img image {
  width: 80rpx;
  height:80rpx;
  vertical-align: top;
  border-radius: 50%;
}

.ranking-me .ranking-me-left-name {
  display: inline-block;
  margin-left: 40rpx;
  margin-top: 20rpx;
  font-weight: bold;
}

.ranking-me .ranking-me-left-name view:nth-child(1) {
  display: inline-block;
  font-size: 28rpx;
  color: #555;
  margin-top: 17rpx;
  width: 220rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.ranking-me .ranking-me-left-name view:nth-child(2) {
  font-size: 24rpx;
  color: #999;
}

.ranking-me-right {
  float: right;
  margin-top: 15rpx;
}

.ranking-me-center {
  float: right;
  margin-top: 30rpx;
}

.ranking-me-center .ranking-me-center-num {
  display: inline-block;
  margin-right: 80rpx;
}

.ranking-me-center .ranking-me-center-num text:nth-child(1) {
  color: #3ACC88;
  font-size: 38rpx;
  margin-right: 5rpx;
}
.ranking-me-center .ranking-me-center-num text:nth-child(2) {
  color:#999;
  font-size: 24rpx;
}

.ranking-me-right .ranking-me-right-img {
  display: inline-block;
  margin-right: 40rpx;
  text-align: center;
}

.ranking-me-right .ranking-me-right-img text {
  display: block;
  color:#999;
  font-size: 24rpx;
}

.ranking-me-right .ranking-me-right-img image {
  width: 30rpx;
  height: 28rpx;
}

.ranking-list {
  /* margin-top: 20rpx; */
  border-bottom: 1rpx solid #f2f3f5;
}

.ranking-me .ranking-medlas {
  display: inline-block;
  margin-left: 30rpx;
  vertical-align: top;
  width: 48rpx;
  height: 64rpx;
  line-height: 128rpx;
  text-align: center;
  font-size: 28rpx;
  color: #555;
}

.ranking-list .ranking-medlas {
  display: inline-block;
  margin-left: 30rpx;
  vertical-align: top;
  width: 50rpx;
  height: 64rpx;
  line-height: 128rpx;
  text-align: center;
  font-size: 28rpx;
  color: #555;
}

.ranking-list .ranking-medlas image {
  width: 48rpx;
  height: 64rpx;
  vertical-align: top;
  margin-top: 30rpx;
}

.ranking-list .ranking-list-left-img {
  display: inline-block;
  margin-left: 22rpx;
  vertical-align: top;
  margin-top: 20rpx;
}

.ranking-list .ranking-list-left-img image {
  width: 80rpx;
  height:80rpx;
  vertical-align: top;
  border-radius: 50%;
}

.ranking-list .ranking-list-left-name {
  display: inline-block;
  margin-left: 40rpx;
  margin-top: 20rpx;
}

.person-null {
  width: 100%;
  margin-top: 200rpx;
  text-align: center;
  font-size: 28rpx;
  color: #999;
}

.person-null image {
  width: 400rpx;
  vertical-align: middle;
}

.information-text {
  font-size: 28rpx;
  color: #999;
  text-align: center;
}

.information-login {
  margin-top: 30rpx;
  width: 60%;
  margin: 0 atuo;
  background: #3ACC88;
  border-radius: 100rpx;
  color: #fff;
}

.ranking_question {
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  display: inline-block;
  color: #fff;
  background: #b3b3b3;
  border-radius:50%;
}



.ys-mask{
  position: fixed;
  z-index: 1000;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6);
}
.ys-dialog{
  position: fixed;
  z-index: 5000;
  width: 85%;
  max-width: 300px;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background-color: #FFFFFF;
  text-align: center;
  border-radius: 16rpx;
  overflow: hidden;
}
.ys-dialog-title{
  padding: 50rpx 100rpx 0 100rpx;
  color:#212121;
  font-size:36rpx;
  font-family:PingFang-SC-Bold;
  font-weight: 400;
}
.ys-dialog-content{
  padding: 20rpx 40rpx 0 40rpx;
  min-height: 40px;
  font-size: 28rpx;
  word-wrap: break-word;
  word-break: break-all;
  color: #555555;
  text-align: left;
  line-height: 46rpx;
}

.ys-dialog-content view {
  margin-bottom: 10rpx;
}

.ys-dialog-bottom{
  margin-top: 10rpx;
  margin-bottom: 50rpx;
}

.ys-dialog-bottom button {
  box-shadow: 0 0 30rpx #3ACC88;
  background: #3ACC88;
  color: #fff;
  border-radius: 44rpx;
  font-size: 35rpx;
  font-weight: bold;
  width: 430rpx;
}

.habit-gender-man {
  color: #3FB8E8;
  font-size: 24rpx;
}

.habit-gender-women {
  color: #EF609A;
  font-size: 24rpx;
}