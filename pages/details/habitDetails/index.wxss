}


.weui-panel {
  margin: 30rpx 30rpx 0 30rpx;
  border-radius: 10rpx;
  background: #f2f3f5;
}

.weui-panel:before,
.weui-panel:after {
  border-width: 0;
}

.weui-panel__head {
  padding: 20rpx;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  border-bottom: 1rpx solid #f2f3f5;
  background: #fff;
}

.weui-panel-head__bd {
  position: relative;
  flex: 1;
  font-size: 30rpx;
  padding-left: 18rpx;
}

.weui-panel-head__bd::before{
  content: " ";
  position: absolute;
  left: 0;
  top: 50%;
  margin-top: -15rpx;
  width: 6rpx;
  height: 30rpx;
  background-color: #3ACC88;
}

.weui-panel-head__ft {
  font-size: 26rpx;
  color: #999;
}

.weui-grid {
  width: 25%;
  border-width: 0;
  padding: 30rpx 30rpx;
}

.weui-grid__icon {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}

.weui-grid__label {
  font-size: 26rpx;
}


.weui-information {
  margin-top: 20rpx;
