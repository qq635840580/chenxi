}

.habit-details-nav-top-btn {
  flex:1;
  margin-top: 15rpx;
}

.habit-details-nav-top-btn image{
  display: block;
  margin: 0 auto;
  width: 100rpx;
  height: 100rpx;
}

.habit-details-nav-top-btn view {
  text-align: center;
  font-size: 30rpx;
  color:#555;
  font-weight: bold;
}

.habit-details-nav-line {
  background: #F2F3F5;
  height: 1rpx;
  margin: 10rpx 0;
}

.habit-details-nav-bottom{
  display: flex;
}

.habit-details-nav-bottom view {
  flex: 1;
  text-align: center;
  font-size: 30rpx;
  color:#555;
  margin-bottom: 10rpx;
}

.habit-details-nav-bottom view:nth-child(1) {
  border-right: 1px solid #F2F3F5;
}

.habit-details-text {
  margin-top: 100rpx;
